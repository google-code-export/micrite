<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="micrite-crm" extends="struts-default" namespace="/crm">
		<action name="index" class="customerAction" method="index">
			<result name="success">/crm/customer.jsp</result>
		</action>
 		<action name="save" class="customerAction" method="save">
			<result name="success">/crm/customer.jsp</result>
		</action>
 		<action name="find" class="customerAction" method="find">
			<result name="success">/crm/customer.jsp</result>
		</action>
 		<action name="edit" class="customerAction" method="edit">
			<result name="success">/crm/customer.jsp</result>
		</action>
    </package>

</struts>
